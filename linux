----------create swap space
dd if=/dev/zero of=/swapfile bs=1M count=24552

chmod 600 /swapfile
mkswap /swapfile
swapon /swapfile
make entry in fstab
/swapfile		swap	swap	defaults	0 0 
-----remove swap
swapoff -v /root/swapfile1
rm /swapfile1
